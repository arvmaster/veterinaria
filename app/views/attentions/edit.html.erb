<h1>Editing article</h1>
 
<%= form_for :attention, url: attention_path(@attention), method: :patch do |f| %>
  <p>
    <%= f.label :descripcion %><br>
    <%= f.text_area :descripcion %>
  </p>
 
 
  <p>
    <%= f.submit "Editar ficha ",class: "btn btn-primary" %>
  </p>
<% end %>
 
<%= link_to 'Back', attentions_path %>